{
  "name": "@withkoji/core",
  "version": "0.0.9",
  "description": "Easily access core pieces of Koij's platform features",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./dist --source-maps --extensions '.ts'",
    "watch": "nodemon --watch ./src --ext ts --exec babel ./src --out-dir ./dist --source-maps --extensions '.ts'",
    "test": "jest ./src",
    "check-types": "tsc",
    "prepare": "npm run build & npm run generate-declarations",
    "generate-declarations": "tsc --emitDeclarationOnly",
    "watch-declarations": "nodemon --watch ./src --ext ts --exec npm run generate-declarations",
    "doc": "typedoc src",
    "json-doc": "typedoc src --json test.json"
  },
  "files": [
    "/dist"
  ],
  "homepage": "https://github.com/madewithkoji/koji-core",
  "bugs": "https://github.com/madewithkoji/koji-core/issues",
  "author": "diddy <seanelliot@withkoji.com>",
  "repository": "github:madewithkoji/koji-core",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.20",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.20.14",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@types/express": "^4.17.11",
    "axios": "^0.21.1",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "isomorphic-ws": "^4.0.1",
    "qs": "^6.9.6",
    "sockette": "^2.0.6",
    "uuid": "^8.3.2",
    "ws": "^7.4.2"
  }
}
